{
    "version": 2,
    "name": "websocket-server",
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js"
        },
        {
            "src": "/sockets/controller.js",
            "dest": "/sockets/controller.js"
        },
        {
            "src": "/public/(.*)",
            "dest": "/public/static/$1"
        }
    ]
}